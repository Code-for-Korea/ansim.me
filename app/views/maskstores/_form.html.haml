= form_for @maskstore do |f|
  - if @maskstore.errors.any?
    #error_explanation
      %h2= "#{pluralize(@maskstore.errors.count, "error")} prohibited this maskstore from being saved:"
      %ul
        - @maskstore.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :type
    = f.text_field :type
  .field
    = f.label :code
    = f.text_field :code
  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :address
    = f.text_field :address
  .field
    = f.label :latitude
    = f.text_field :latitude
  .field
    = f.label :longitude
    = f.text_field :longitude
  .actions
    = f.submit 'Save'
